<!DOCTYPE html>
<html lang="en" data-bs-core="modern">
	<head>
		<%- include('partials/head') %>
	</head>
	<body data-halfmoon="dark-mode">
		<div class="serverwrapper app-bg">
			<%- include('partials/header', { servers: servers }) %> <% if (typeof
			currentServer !== 'undefined') { %> <%- include('partials/sidepanel', {
			server: currentServer }) %> <% } %> <%- include('partials/activespace') %>
			<% if (showMembers) { %> <%- include('partials/members', { members:
			members }) %> <% } %>
		</div>
		<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.min.js"></script>
	</body>
</html>
